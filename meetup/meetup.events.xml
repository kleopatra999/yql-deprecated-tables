<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd" https="true">
	<meta>
		<author>Sergey Chernyshev</author>
		<documentationURL>http://www.meetup.com/meetup_api/docs/#readmethods</documentationURL>
		<sampleQuery>select * from {table} where key="..." and zip="10016"</sampleQuery>
	</meta>
	<bindings>
		<select itemPath="results" produces="XML">
			<urls>
				<url><![CDATA[http://api.meetup.com/events.xml?key={key}]]></url>
			</urls>
			<inputs>
				<key id="key" type="xs:string" paramType="path" required="true"/>
				<key id="zip" type="xs:integer" paramType="query" required="false"/>
				<key id="group_id" type="xs:integer" paramType="query" required="false"/>
				<key id="country" type="xs:string" paramType="query" required="false"/>
				<key id="city" type="xs:string" paramType="query" required="false"/>
				<key id="state" type="xs:string" paramType="query" required="false"/>
				<key id="lat" type="xs:string" paramType="query" required="false"/>
				<key id="lon" type="xs:string" paramType="query" required="false"/>
				<key id="topic" type="xs:string" paramType="query" required="false"/>
				<key id="id" type="xs:integer" paramType="query" required="false"/>
				<key id="groupnum" type="xs:string" paramType="query" required="false"/>
				<key id="group_urlname" type="xs:string" paramType="query" required="false"/>
				<key id="radius" type="xs:string" paramType="query" required="false"/>
				<key id="after" type="xs:string" paramType="query" required="false"/>
				<key id="before" type="xs:string" paramType="query" required="false"/>
				<key id="order" type="xs:string" paramType="query" required="false"/>
				<key id="desc" type="xs:bool" paramType="query" required="false"/>
			</inputs>
<!--			<paging model="page">
				<start id="offset" default="0" />
				<pagesize id="page" max="200" />
				<total default="200" />
			</paging> -->
		</select>
	</bindings>
</table>

